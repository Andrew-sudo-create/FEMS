      <div class="contact-form">
        <form class="" action="/send" method="post">
          <div class="name">
            <input class="input-field" type="text" name="firstName" placeholder="First Name" value="" required>
            <input class="input-field" type="text" name="lastName" placeholder="Last Name" value="" required>
            <input id="contactEmailValidate" onclick="validate()" class="input-field" type="email" name="eMail" placeholder="E-Mail" value="" required>
            <input class="input-field" type="number" name="phone" placeholder="Phone" value="" required>
          </div>
          <div class="numbers">
            <textarea rows="2" cols="35" name="message" placeholder="Your Message Here..." required></textarea>
            <button class="" type="submit" name="button">Send Message</button>
          </div>
        </form>
      </div>
    </div>
  </div>
